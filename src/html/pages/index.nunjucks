<!-- index file -->
{% extends "layout.nunjucks" %}

{% block content %}

<h1>Home time</h1>

<div class="gallery">
    <!-- Loops through "images" array -->
    {% for image in images %}
    <div class="gallery__item">
        <img src="{{image.src}}" alt="{{image.alt}}">
    </div>
    {% endfor %}
</div>

<!-- static -->
{% include "partials/navigation.nunjucks" %}
<br />

<!-- dynamic -->
{% import 'macros/nav-macro.nunjucks' as nav %}
{{nav.active('home')}}

{% endblock %}
